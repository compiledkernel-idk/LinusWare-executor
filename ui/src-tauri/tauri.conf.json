{
    "productName": "linusware",
    "version": "0.1.0",
    "identifier": "com.linusware.executor",
    "build": {
        "frontendDist": "../src/"
    },
    "app": {
        "withGlobalTauri": true,
        "windows": [
            {
                "title": "LINUSWARE",
                "width": 1100,
                "height": 720,
                "minWidth": 900,
                "minHeight": 600,
                "resizable": true,
                "fullscreen": false,
                "decorations": true,
                "transparent": false,
                "center": true
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/icon.png"
        ]
    }
}